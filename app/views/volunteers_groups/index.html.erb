        <%# <td><%= link_to 'Show', volunteers_group %1></td> %>
        <%# <td><%= link_to 'Edit', edit_volunteers_group_path(volunteers_group) %1></td> %>
        <%# <td><%= link_to 'Destroy', volunteers_group, method: :delete, data: { confirm: 'Are you sure?' } %1></td> %>
      </tr>
    <% end %>
  </tbody>
</table>
