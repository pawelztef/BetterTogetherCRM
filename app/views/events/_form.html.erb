<%= simple_form_for @event, remote: true do |f| %>
  <%= f.error_notification %>

  <%= f.input :start, as: :string, input_html: { class: "datepicker", id: "startTime" } %> 
  <%= f.input :end, as: :string, input_html: { class: "datepicker", id: "endTime" } %> 

  <%= f.button :submit %>

<% end %>
