    <!--COMPLETED ACTIONS DONUTS CHART-->
    <h3>INCOMING EVENTS</h3>

    <!-- First Action -->
    
    <% @events.each do |event| %>
    <div class="desc">
      <div class="thumb">
        <span class="badge bg-theme"><i class="fa fa-clock-o"></i></span>
      </div>
      <div class="details">
        <p><muted> <%= distance_of_time_in_words(Date.current, event.start) + " to go" %> </muted><br/>
        <a href="#"> <%= event.title %> </a> of type <%= event_type(event) %>.<br/>
        <%= stringify_event_dates event %>
        </p>
      </div>
    </div>
    <% end %> 

