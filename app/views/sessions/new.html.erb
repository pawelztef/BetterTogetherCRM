<div class="container login">
  <div class="row">
    <div class="col-center col-sm-3">
      <h1>Login</h1>
      <%= simple_form_for :user, {url: sessions_path} do |f| %>
        <%= f.input :email %> 
        <%= f.input :password %> 
        <%= f.button :submit, "Log In" %> 
      <% end %> 
    </div>
  </div>
</div>

