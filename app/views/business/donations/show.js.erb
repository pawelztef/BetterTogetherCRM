$('body').append('<%= j render("show") %>');
$("#donationShowModal").modal('show');
$(".closeDonationShowModal").on("click", function() {
$("#donationShowModal").modal('hide');
});
$('body').on('hidden.bs.modal', '.modal', function (e) {
  $(e.target).removeData('bs.modal');
  $('.modal').remove();
});


