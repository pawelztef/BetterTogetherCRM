<div class="modal fade" id="accountEditModal">
  <div class="modal-dialog modal-lg">
    <div class="modal-content">
      <div class="modal-body">
        <div class="row">
          <div class="col-xs-12">
            <div class="containter">
              <div class="row">
                <div class="col-sm-12">
                  <button type="button" class="closeAccountEditModal close">x</button>
                </div>
              </div>
              <br />
              <div class="row">

                <div class="col-md-12">

                  <h1>Editing Account: <%= @account.name_was %></h1>

                  <%= render 'form' %>
                </div>

              </div>
              <div class="row">
                <div class="col-sm-12">
                  <button type="button" class="closeAccountEditModal btn btn-default">close</button>
                </div>
              </div>
            </div>
            <br />
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

