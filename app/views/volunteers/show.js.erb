$('body').append('<%= j render("show") %>');
$("#volunteerShowModal").modal('show');
$(".closeVolunteer").on("click", function() {
  $("#volunteerShowModal").modal('hide');
});
$('body').on('hidden.bs.modal', '.modal', function (e) {
  $(e.target).removeData('bs.modal');
  $('.modal').remove();
});

